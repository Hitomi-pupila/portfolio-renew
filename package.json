  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hitomi-pupila/sample/issues"
  },
  "homepage": "https://github.com/Hitomi-pupila/sample#readme",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "browser-sync": "^3.0.3",
    "eslint": "^9.21.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^11.0.0",
    "postcss-sort-media-queries": "^5.2.0",
    "sass": "^1.85.0",
    "textlint": "^14.4.2",
    "textlint-rule-no-hankaku-kana": "^2.0.1",
    "textlint-rule-no-kangxi-radicals": "^0.2.2",
    "textlint-rule-no-nfd": "^2.0.2"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "rimraf": "^6.0.1"
  }
}
